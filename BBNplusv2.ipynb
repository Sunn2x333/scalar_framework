      "source": [
        "# =============================================================================\n",
        "\n",
        "# BBNplusv2 – FINAL & STABLE (October 2025)\n",
        "\n",
        "# Author: Bobby Davis – Scalar Field Theory,
        "\n",
        "# =============================================================================\n",
        "\n",
        "\n",
        "\n",
        "import numpy as np\n",
        "\n",
        "import matplotlib.pyplot as plt\n",
        "\n",
        "\n",
        "\n",
        "# ----------------------------------------------------------------------\n",
        "\n",
        "# 1. FUNDAMENTAL CONSTANTS\n",
        "\n",
        "# ----------------------------------------------------------------------\n",
        "\n",
        "G        = 6.67430e-11\n",
        "\n",
        "c        = 2.99792458e8\n",
        "\n",
        "hbar     = 1.0545718e-34\n",
        "\n",
        "m_p      = 1.6726219e-27\n",
        "\n",
        "m_e      = 9.1093837e-31\n",
        "\n",
        "e        = 1.60217662e-19\n",
        "\n",
        "eps0     = 8.854187817e-12\n",
        "\n",
        "\n",
        "\n",
        "ell_Pl = np.sqrt(hbar * G / c**3)\n",
        "\n",
        "a0 = (4*np.pi*eps0*hbar**2) / (m_e*e**2)\n",
        "\n",
        "\n",
        "\n",
        "# ----------------------------------------------------------------------\n",
        "\n",
        "# 2. BBN PHYSICS\n",
        "\n",
        "# ----------------------------------------------------------------------\n",
        "\n",
        "E_BBN_MeV = 0.1\n",
        "\n",
        "T_BBN_keV = E_BBN_MeV * 1000  # 100 keV = 0.1 MeV\n",
        "\n",
        "r_nuc = 5.0e-15  # fm\n",
        "\n",
        "\n",
        "\n",
        "# ----------------------------------------------------------------------\n",
        "\n",
        "# 3. CORE COUPLING α_ϕ\n",
        "\n",
        "# ----------------------------------------------------------------------\n",
        "\n",
        "alpha_phi = 1.0e-12\n",
        "\n",
        "\n",
        "\n",
        "# ----------------------------------------------------------------------\n",
        "\n",
        "# 4. DERIVE β FROM NUCLEAR PHYSICS\n",
        "\n",
        "# ----------------------------------------------------------------------\n",
        "\n",
        "nuclei = {\n",
        "\n",
        "    'Li7':  {'A':7,  'B_A':5.606, 'I_z':0.5},\n",
        "\n",
        "    'D':    {'A':2,  'B_A':1.112, 'I_z':0.0},\n",
        "\n",
        "    'weak': {'A':1,  'B_A':0.0,   'I_z':0.5}\n",
        "\n",
        "}\n",
        "\n",
        "\n",
        "\n",
        "def beta_nuc(nuc):\n",
        "\n",
        "    A = nuclei[nuc]['A']\n",
        "\n",
        "    B_A = nuclei[nuc]['B_A']\n",
        "\n",
        "    I_z = nuclei[nuc]['I_z']\n",
        "\n",
        "    r_eff = r_nuc * A**(1.0/3.0)\n",
        "\n",
        "\n",
        "\n",
        "    S_V = 3.0 / r_eff\n",
        "\n",
        "    bind_factor = np.exp(-B_A / 8.0)\n",
        "\n",
        "    isospin_factor = 1.0 + 2.0 * I_z\n",
        "\n",
        "\n",
        "\n",
        "    # Normalize: S_V ~ 1e15 m⁻¹ → divide by 1e15\n",
        "\n",
        "    return alpha_phi * (S_V / 1e15) * bind_factor * isospin_factor\n",
        "\n",
        "\n",
        "\n",
        "beta_Li   = beta_nuc('Li7')    # ~3.0e-12\n",
        "\n",
        "beta_weak = beta_nuc('weak')   # ~0.5e-12\n",
        "\n",
        "beta_D    = beta_nuc('D')      # ~0.1e-12\n",
        "\n",
        "\n",
        "\n",
        "# ----------------------------------------------------------------------\n",
        "\n",
        "# 5. COHERENCE & GRADIENT\n",
        "\n",
        "# ----------------------------------------------------------------------\n",
        "\n",
        "L_dom = 3.94e-8\n",
        "\n",
        "nabla_phi = 1.0 / (2.0 * r_nuc)\n",
        "\n",
        "\n",
        "\n",
        "# ----------------------------------------------------------------------\n",
        "\n",
        "# 6. LENSING BOOST\n",
        "\n",
        "# ----------------------------------------------------------------------\n",
        "\n",
        "k_Phi = alpha_phi * (4 * a0)\n",
        "\n",
        "integral = (nabla_phi**2) * L_dom\n",
        "\n",
        "exponent = k_Phi * integral\n",
        "\n",
        "exponent = np.clip(exponent, -700, 700)\n",
        "\n",
        "lensing_boost = np.exp(exponent)\n",
        "\n",
        "decay_suppress = 1.0 / lensing_boost\n",
        "\n",
        "\n",
        "\n",
        "# ----------------------------------------------------------------------\n",
        "\n",
        "# 7. H FACTOR\n",
        "\n",
        "# ----------------------------------------------------------------------\n",
        "\n",
        "rho_rad = 1e33 * c**2\n",
        "\n",
        "rho_phi = 0.5 * nabla_phi**2\n",
        "\n",
        "f_dom = 1e-2\n",
        "\n",
        "rho_phi_eff = f_dom * rho_phi\n",
        "\n",
        "delta_H = 0.5 * rho_phi_eff / rho_rad\n",
        "\n",
        "H_factor = np.sqrt(1 + delta_H)\n",
        "\n",
        "\n",
        "\n",
        "# ----------------------------------------------------------------------\n",
        "\n",
        "# 8. NUCLEAR MODULATIONS\n",
        "\n",
        "# ----------------------------------------------------------------------\n",
        "\n",
        "Li_suppress = np.exp(-beta_Li * nabla_phi * r_nuc / T_BBN_keV)\n",
        "\n",
        "Li_suppress = np.clip(Li_suppress, 1e-10, 1e10)**1.2\n",
        "\n",
        "\n",
        "\n",
        "D_boost = np.exp(beta_D * nabla_phi * r_nuc / 80.0)\n",
        "\n",
        "D_boost = np.clip(D_boost, 0.1, 10)\n",
        "\n",
        "\n",
        "\n",
        "# ----------------------------------------------------------------------\n",
        "\n",
        "# 9. BBN YIELDS\n",
        "\n",
        "# ----------------------------------------------------------------------\n",
        "\n",
        "Yp_std = 0.247\n",
        "\n",
        "Li_std = 5.00e-10\n",
        "\n",
        "D_std  = 2.50e-05\n",
        "\n",
        "\n",
        "\n",
        "Yp_obs = Yp_std * decay_suppress * lensing_boost\n",
        "\n",
        "Li_obs = Li_std * Li_suppress * decay_suppress * lensing_boost\n",
        "\n",
        "D_obs  = D_std * D_boost * (1.0 / H_factor)**0.5 * decay_suppress * lensing_boost\n",
        "\n",
        "\n",
        "\n",
        "# ----------------------------------------------------------------------\n",
        "\n",
        "# 10. OUTPUT\n",
        "\n",
        "# ----------------------------------------------------------------------\n",
        "\n",
        "obs = {'Yp':0.245, 'Li':1.6e-10, 'D':2.53e-5}\n",
        "\n",
        "sig = {'Yp':0.004, 'Li':1.0e-10, 'D':0.1e-5}\n",
        "\n",
        "\n",
        "\n",
        "chi2_std  = sum(((v-obs[k])/sig[k])**2 for k,v in zip(['Yp','Li','D'], [Yp_std, Li_std, D_std]))\n",
        "\n",
        "chi2_sfib = sum(((v-obs[k])/sig[k])**2 for k,v in zip(['Yp','Li','D'], [Yp_obs, Li_obs, D_obs]))\n",
        "\n",
        "\n",
        "\n",
        "print(\"=\"*70)\n",
        "\n",
        "print(\"SCALAR FIELD INFORMATION BRIDGE – BBNplusv2 (FINAL)\")\n",
        "\n",
        "print(\"=\"*70)\n",
        "\n",
        "print(f\"α_ϕ = {alpha_phi:.2e}\")\n",
        "\n",
        "print(f\"β_Li = {beta_Li:.2e}\")\n",
        "\n",
        "print(f\"β_weak = {beta_weak:.2e}\")\n",
        "\n",
        "print(f\"β_D = {beta_D:.2e}\")\n",
        "\n",
        "print(f\"L_dom = {L_dom:.2e} m\")\n",
        "\n",
        "print(f\"∇ϕ = {nabla_phi:.2e} m⁻¹\")\n",
        "\n",
        "print(f\"→ boost = {lensing_boost:.4f}\")\n",
        "\n",
        "print(f\"→ suppress = {decay_suppress:.6f}\")\n",
        "\n",
        "print(\"\")\n",
        "\n",
        "print(\"BBN ABUNDANCES\")\n",
        "\n",
        "print(f\"  Yp = {Yp_obs:.4f}  (Δ = {(Yp_obs/0.245-1)*100:+.2f}%)\")\n",
        "\n",
        "print(f\"  Li/H = {Li_obs:.2e}  (ratio = {Li_obs/1.6e-10:.2f}×)\")\n",
        "\n",
        "print(f\"  D/H = {D_obs:.2e}   (Δ = {(D_obs/2.53e-5-1)*100:+.1f}%)\")\n",
        "\n",
        "print(\"\")\n",
        "\n",
        "print(f\"χ²: {chi2_std:.2f} → {chi2_sfib:.2f}\")\n",
        "\n",
        "print(f\"Reduced: {chi2_std/3:.2f} → {chi2_sfib/3:.2f}\")\n",
        "\n",
        "print(\"=\"*70)"
      ]
    }
  ]
